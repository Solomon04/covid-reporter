{
    "name": "solomon04/covid-reporter",
    "description": "Covid Reporter.",
    "keywords": ["covid", "laravel zero", "covid-reporter", "cli"],
    "homepage": "https://github.com/Solomon04/covid-reporter",
    "type": "project",
    "authors": [
        {
            "name": "Solomon Antoine",
            "email": "antoinesolomon5@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^6.3.1",
        "illuminate/http": "^7.0",
        "laminas/laminas-text": "^2.7",
        "laravel-zero/framework": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5.2",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["covid"]
}
